    "colors": "^1.4.0",
    "csv-parse": "=4.9.1",
    "fast-glob": "^3.2.12",
    "fp-ts": "=2.11.8",
    "graphql": "^16.6.0",
    "graphql-request": "^5.0.0",
    "io-ts": "=2.2.16",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "yargs-parser": "^21.1.1"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.37",
    "@types/chai": "^4.3.3",
