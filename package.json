    "lint": "yarn pnpify eslint src --ext .ts",
    "prepublish": "yarn build",
    "test": "yarn pnpify mocha './src/**/*.test.ts'"
  },
  "dependencies": {
    "@transcend-io/privacy-types": "^4.11.1",
